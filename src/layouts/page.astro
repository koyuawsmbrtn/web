---
import Navigation from "../components/navigation.astro";
import Layout from "../layouts/base.astro";
import Footer from "../components/footer.astro";
const { title } = Astro.props;
---
<Layout title={title}>
	<Navigation />
	<div class="article-meta">
		<h1>{title}</h1>
	</div>
	<br>
	<slot />
	<Footer />
</Layout>